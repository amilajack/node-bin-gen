{
  "name": "node-bin-gen",
  "version": "9.0.2",
  "description": "Generate a node binary package",
  "author": "Amila Welihinda <amilajack@gmail.com>",
  "main": "index.js",
  "bin": "index.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/amilajack/node-bin-gen"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "dependencies": {
    "end-of-stream": "^1.4.1",
    "make-fetch-happen": "^4.0.1",
    "pump": "^3.0.0",
    "rimraf": "^2.6.2",
    "verror": "^1.10.0",
    "yargs": "^12.0.2"
  },
  "scripts": {
    "start": "./index.js 10.8.0 --package-name @amilajack/node",
    "test": "yarn start"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 8
    },
    "rules": {
      "valid-typeof": "error",
      "no-undef": "error",
      "no-unused-vars": "error",
      "no-process-exit": 0,
      "no-shadow": 0,
      "strict": 0,
      "eqeqeq": 0,
      "curly": 0
    },
    "env": {
      "node": true,
      "es6": true
    }
  }
}
